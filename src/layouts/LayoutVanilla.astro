---
interface Props {
	title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="description" content="Astro description" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<title>{title}</title>
	</head>
	<body>
		<div class="header">
			<div class="topHeader">
				<div class="topHeaderLeft">					
					<p class="whiteText">
						<a href="">Descargar</a> | Síguenos en
						<a href="">
							<img class="hasWhiteFiler" src="data:image/png;base64,"/>
						</a>
						<a href="">
							<img class="hasWhiteFiler" src="data:image/png;base64,"/>
						</a>
					</p>
				</div>
				<div class="topHeaderRight">
					<p class="whiteText topHeaderRightHolder">
						<a class="hasButtonAndText" href="">
							<img class="hasWhiteFiler" src="data:image/png;base64,"/>
							<span>Notificaciones</span>
						</a>
						<a class="hasButtonAndText" href="">
							<img class="hasWhiteFiler" src="data:image/png;base64,"/>
							<span>Ayuda</span>
						</a>
						<a class="hasButtonAndText" href="">
							<img class="hasWhiteFiler" src="data:image/png;base64,"/>
							<span> Español-Colombia v</span>
						</a>
						<a href="">Registrarse</a> | <a href="">Iniciar Sesión</a>
					</p>
				</div>
			</div>
			<div class="bottomHeader">
				<div class="logoDiv">

				</div>
				<div class="moreButtonsDiv">

				</div>
				<div class="shopKartDiv">
					<img class="hasWhiteFiler" src="data:image/png;base64,"/>
				</div>
			</div>
		</div>
		<!-- <div class="advertisements">

		</div>
		<div class="optionButtons">

		</div>
		<div class="firstBuy">

		</div> -->
		<slot />
	</body>
</html>
<style is:global>
	:root {
		--accent: 136, 58, 234;
		--accent-light: 224, 204, 250;
		--accent-dark: 49, 10, 101;
		--accent-gradient: linear-gradient(
			45deg,
			rgb(var(--accent)),
			rgb(var(--accent-light)) 30%,
			white 60%
		);
	}
	html {
		font-family: system-ui, sans-serif;
		background: #13151a;
		background-size: 224px;
	}
	body {
		padding: 0;
		margin: 0;
	}
	code {
		font-family:
			Menlo,
			Monaco,
			Lucida Console,
			Liberation Mono,
			DejaVu Sans Mono,
			Bitstream Vera Sans Mono,
			Courier New,
			monospace;
	}
	.topHeader {
		display: flex;
		justify-content: space-between;
	}
	.whiteText {
		color: white;
	}
	a {
		text-decoration: none;
		color:inherit;
	}
	.hasWhiteFiler {
		filter: invert();
	}
	.header {
		background-color: rgb(245, 62, 45);
		padding: 0 4%;
	}
	.topHeaderLeft {
		display: flex;
		justify-content: left;
		align-items: center;
		min-height: 40px;		
	}
	.hasButtonAndText {
		display: flex;
		align-items: center;
		justify-content: center;
	}
	.topHeaderRightHolder {
		display: flex;
		justify-content: space-around;
		align-items: center;
		min-height: 40px;
		gap: 20px;
	}
</style>
